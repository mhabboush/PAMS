<div
  class="modal fade"
  id="revs"
  tabindex="-1"
  role="dialog"
  aria-labelledby="revslLabel"
  aria-hidden="true"
>
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="revsLabel">
          Reviews
        </h5>
        <button
          type="button"
          class="close"
          data-dismiss="modal"
          aria-label="Close"
        >
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <table class="table table-striped">
          <tbody>
            <% data.reviews.forEach(r => { %>
            <tr>
              <td><%= r %></td>
            </tr>
            <% }) %>
          </tbody>
        </table>
      </div>
      <div class="modal-footer"></div>
    </div>
  </div>
</div>
